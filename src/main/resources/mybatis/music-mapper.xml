<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="music">

	


	<insert id="insert">
		INSERT INTO MUSIC 
		VALUES(MUSIC_NO_SEQ.NEXTVAL, #{member_id}, #{member_nickname}, #{music_title}, #{music_content}, #{music_savename}, #{music_realname}, SYSDATE)
	</insert>

	<update id="update">
		UPDATE MUSIC
		SET MUSIC_TITLE = #{music_title}, MUSIC_CONTENT = #{music_content}, MUSIC_SAVENAME = #{music_savename}
		WHERE MEMBER_ID = #{member_id}
	</update>

	<delete id="deletemusic">
		DELETE
		FROM MUSIC
		WHERE MUSIC_NO = #{music_no}
	</delete>
</mapper>
